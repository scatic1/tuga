             xmlns:local="clr-namespace:ButtonCircle.FormsPlugin.Abstractions;assembly=ButtonCircle.FormsPlugin.Abstractions"
             x:Class="needl.MainPage">

    <StackLayout>
        <local:CircleButton 
        FontIcon="Material"
        Icon="md-directions-walk" 
        FontSize="50" TextColor="White" 
        HeightRequest="83" WidthRequest="83" 
            
      
        BackgroundColor="#5B5B5D">
