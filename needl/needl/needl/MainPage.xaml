        <local:CircleButton 
        FontIcon="FontAwesome"
        Icon="fa-briefcase" 
       FontSize="50" TextColor="White" 
        HeightRequest="83" WidthRequest="83" 
       
        BackgroundColor="#5B5B5D">
        </local:CircleButton>
    </StackLayout>

</ContentPage>
